# Supabase connection details
SUPABASE_URL=YOUR_SUPABASE_URL
SUPABASE_SERVICE_KEY=your_service_key_here

# Firecrawl API key
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# GitHub API token (optional, for rate limit improvements and private repos)
GITHUB_API_TOKEN=your_github_token_here

# README Processing
# Set to 'true' to enable GitHub README parsing and extraction
PROCESS_READMES=false

# Crawler settings
CRAWL_BATCH_SIZE=20
CRAWL_DELAY_MS=500
